local Coasting = loadstring(game:HttpGet("https://raw.githubusercontent.com/cheesynob39/Coasting/main/Source.lua"))()

local Tab = Coasting:CreateTab("UNO")
local Tab1 = Tab:CreateSection("Main")
local Tab2 = Tab:CreateSection("Main")

Tab1:CreateToggle("Auto 6 Slap", function(bool)
   _G.Auto6Slap = bool
while _G.Auto6Slap do
game:GetService("ReplicatedStorage").Remotes.SlapOnSix:FireServer()
task.wait()
end
end)

Tab1:CreateToggle("Auto Official", function(bool)
   _G.Call = bool
while _G.Call do
game:GetService("ReplicatedStorage").Remotes.CallOfficial:FireServer()
task.wait()
end
end)

